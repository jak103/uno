{
    "family": "uno-task",
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "name": "uno-container",
            "image": "ubuntu-latest",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                }, {
                    "containerPort": 443,
                    "hostPort": 443,
                }
            ],
            "entryPoint": [
                "/uno/uno"
            ],
            "linuxParameters": {
                "capabilities": {},
                "devices": [],
                "initProcessEnabled": true,
                "sharedMemorySize": 0,
                "tmpfs": [],
                "maxSwap": 0,
                "swappiness": 0
            },
            "user": "root",
            "workingDirectory": "",
            "disableNetworking": false,
            "privileged": true,
            "readonlyRootFilesystem": true,
            "dnsServers": [
                "8.8.8.8",
                "8.8.4.4"
            ]
        }
    ],
    "volumes": [
        {
            "name": "uno-volume",
            "host": {
                "sourcePath": "/"
            },
            "dockerVolumeConfiguration": {
                "scope": "shared",
                "autoprovision": true,
            },
            "efsVolumeConfiguration": {
                "fileSystemId": "",
                "rootDirectory": "",
                "transitEncryption": "DISABLED",
                "transitEncryptionPort": 0,
                "authorizationConfig": {
                    "accessPointId": "",
                    "iam": "DISABLED"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "1",
    "memory": "1 gb",
    "pidMode": "host",
    "ipcMode": "task"
}
